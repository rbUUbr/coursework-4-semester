<div id="demo">
  <div class="table-responsive-vertical shadow-z-1">
    <table class="table table-hover table-mc-light-blue">
      <thead>
        <tr>
          <td>
            Subject
          </td>
          <td>
            Date
          </td>
          <td>
            Time
          </td>
          <td>
            Actions
          </td>
        </tr>
      </thead>
      <tbody>
      <% @lab_queues.each do |lab_queue| %>
        <tr>
          <td>
            <%= lab_queue.schedule.subject %>
          </td>
          <td>
            <%= lab_queue.date %>
          </td>
          <td>
            <%= lab_queue.schedule.time %>
          </td>
          <td>
            <%= link_to 'Registrate', lab_queue_registrate_on_lab_path(lab_queue.id), 'data-lab_queue_id': "#{lab_queue.id}" %>
          </td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>